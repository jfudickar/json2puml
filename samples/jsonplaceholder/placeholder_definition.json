{
  "description": {
    "displayName": "JSON Placeholder Definition File",
    "description": "Converter definition file to generate plantuml files for jsonplaceholder data files.\nThis file is baed on the definitions of the public REST API at \nhttps://jsonplaceholder.typicode.com/ to show the capabilities of JSON2PUML."
  },
	"defaultOption": "default",
	"options": [
			{
				"option": "compact",
				"definition": {
					"attributeProperties": {
						"operation": "replace",
						"list": [
								"id",
								"*name",
								"status"
						]
					},
					"objectFormats": {
						"baseFormat": {
						}
					}
				}
			},
			{
				"option": "default",
				"definition": {
					"objectFormats": {
						"baseFormat": {
						}
					}
				}
			},
			{
				"option": "detailed",
				"definition": {
					"attributeProperties": [
							"*"
					],
					"objectFormats": {
						"baseFormat": {
						}
					}
				}
			},
			{
				"option": "full",
				"definition": {
					"continueAfterUnhandledObjects": "true",
					"attributeProperties": [
							"*"
					],
					"objectProperties": {
						"operation": "replace",
						"list": [
								{"objectName": "*","generateWithoutIdentifier": "true"}
						]
					},
					"objectFormats": {
						"baseFormat": {
						}
					}
				}
			},
			{
				"option": "relations",
				"definition": {
					"continueAfterUnhandledObjects": "true",
					"attributeProperties": {
						"operation": "replace",
						"list": [
								"id",
								"*name",
								"status"
						]
					},
					"objectProperties": {
						"operation": "replace",
						"list": [
								{"objectName": "*","generateWithoutIdentifier": "true"}
						]
					},
					"objectFormats": {
						"baseFormat": {
						}
					}
				}
			},
			{
				"option": "short",
				"definition": {
					"attributeProperties": {
						"operation": "replace",
						"list": [
								"id",
								"*name",
								"status"
						]
					},
					"objectFormats": {
						"baseFormat": {
						}
					}
				}
			}
	],
	"baseOption": {
		"continueAfterUnhandledObjects": "false",
		"groupDetailObjectsTogether": null,
		"hideDuplicateRelations": null,
		"identifyObjectsByTypeAndIdent": "true",
		"legendShowInfo": null,
		"legendShowObjectFormats": null,
		"legendShowFileInfos": null,
		"outputPath": "output\\<group>\\<option>",
		"outputSuffix": ".<group>.<option>.<detail>",
		"outputFormats":["png","svg"],
		"attributeProperties": [
				"*"
		],
		"objectProperties": [
				"*"
		],
		"objectTypeRenames": [
				"postId=post",
				"albumId=album",
				"userId=user"
		],
		"objectDetailProperties": [
		],
		"groupProperties": [
		],
		"objectIdentifierProperties": [
				"id"
		],
		"relationshipProperties": [
				"postId",
				"albumId",
				"userId"
		],
		"relationshipTypeProperties": [
		],
