				"curlExecuteEvaluation": null,
				"curlFileSuffix": "_${userid}",
				"curlFormatOutput": null,
				"curlOptions": null,
				"curlUrl": "posts?userId=${userid}",
				"splitIdentifier": null,
				"splitInputFile": null,
				"leadingObject": "post",
				"generateOutput": true,
				"curlOutputParameter":  [
						{
							"name": "${postid}",
							"value": "id",
							"maxValues": 2
						}
				]
			},
			{
				"inputFile": "comments.json",
				"curlBaseUrl": null,
				"curlCache": 120,
				"curlExecuteEvaluation": "\"${includecomments}\" = \"y\"",
