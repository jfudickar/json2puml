				"urlEncodeValue": false
			},
			{
				"name": "${filter.title}",
				"prefix": "?search=",
				"value": "${input.title}",
				"suffix": null,
				"valueIfEmpty": null,
				"urlEncodeValue": true
			},
			{
				"name": "${filter}",
				"prefix": null,
				"value": "${filter.id}${filter.title}",
				"suffix": null,
				"valueIfEmpty": null,
				"urlEncodeValue": false
			},
			{
				"name": "${filegoup}",
				"prefix": null,
				"value": "${input.id}${input.title}",
				"suffix": null,
				"valueIfEmpty": null,
				"urlEncodeValue": false
			}
	],
	"summaryFile": "${title}",
	"input": [
			{
				"inputFile": "films.json",
				"leadingObject": "films",
				"generateOutput": true,
				"mandatory": true,
				"curlBaseUrl": "https://swapi.dev/api/films/${filter}",
				"curlCache": 0,
				"curlExecuteEvaluation": null,
				"curlFileSuffix": "_${filegoup}",
				"curlFormatOutput": true,
				"curlOptions": null,
