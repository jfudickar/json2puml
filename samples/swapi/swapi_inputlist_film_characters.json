				"splitIdentifier": null,
				"splitInputFile": false
			},
			{
				"inputFile": "characters.json",
				"leadingObject": "characters",
				"generateOutput": true,
				"mandatory": false,
				"curlBaseUrl": "${characters}",
				"curlCache": 0,
				"curlExecuteEvaluation": "\"${characters}\" <> \"\"",
