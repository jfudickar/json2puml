{
	"description": {
		"displayName": "TV MAZE Show List",
		"description": "This example is based on the TVMAZE API (\"https://www.tvmaze.com/api\").\nIt delivers a defined film with the starships and vehicles of the film.",
		"curlParameter":  [
				{
					"name": "${id}",
					"displayName": "Show ID",
					"description": "Primary id of the Show",
					"mandatory": "true",
					"regularExpression": ""
				}
		]
	},
	"definitionFile": "tvmaze_definition.json",
	"generateDetails": null,
	"generateSummary": null,
	"group": "${id}",
	"detail": null,
	"option": null,
	"job": null,
	"outputFormats":["svg"],
	"outputPath": null,
	"outputSuffix": null,
	"curlBaseUrl": "https://api.tvmaze.com/",
	"curlUrlAddon": null,
	"curlOptions": null,
	"summaryFile": "${name}",
	"input":  [
			{
				"inputFile": "show.json",
				"curlFileSuffix": "_${id}",
				"curlBaseUrl": null,
				"curlFormatOutput": "true",
				"curlUrl": "shows/${id}?embed[]=cast&embed[]=nextepisode",
				"curlOptions": null,
				"curlCache": "0",
				"splitIdentifier": null,
				"splitInputFile": null,
				"leadingObject": "show",
				"generateOutput": "true",
				"curlOutputParameter":  [
						{
							"${character}": "_embedded.cast.character.id"
						},
						{
							"${name}": "name"
						},
						{
							"${person}": "_embedded.cast.person.id"
						}
				]
			},
			{
				"inputFile": "person.json",
				"curlFileSuffix": "_${person}",
				"curlBaseUrl": null,
				"curlFormatOutput": "true",
				"curlUrl": "people/${person}",
				"curlOptions": null,
				"curlCache": "0",
				"splitIdentifier": null,
				"splitInputFile": null,
				"leadingObject": "person",
				"generateOutput": "true",
				"curlOutputParameter":  [
				]
			},
			{
				"inputFile": "character.json",
				"curlFileSuffix": "_${character}",
				"curlBaseUrl": null,
				"curlFormatOutput": "true",
				"curlUrl": "character/${character}",
				"curlOptions": null,
				"curlCache": "0",
				"splitIdentifier": null,
				"splitInputFile": null,
				"leadingObject": "character",
				"generateOutput": "true",
				"curlOutputParameter":  [
				]
			}
	]
}
