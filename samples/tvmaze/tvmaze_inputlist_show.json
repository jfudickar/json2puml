{
	"description": {
		"displayName": "TV MAZE Show List",
		"description": "This example is based on the TVMAZE API (\"https://www.tvmaze.com/api\").\nIt delivers a defined film with the starships and vehicles of the film.",
		"curlParameter":  [
				{
					"name": "${id}",
					"displayName": "Show ID",
					"description": "Primary id of the Show",
					"mandatory": true,
					"regularExpression": null,
					"defaultValue": null
				}
		]
	},
	"definitionFile": "tvmaze_definition.json",
	"generateDetails": null,
	"generateSummary": null,
	"group": "${id}",
	"detail": null,
	"option": null,
	"job": null,
	"jobDescription": "All details around the show \"${name}\"",
	"outputFormats": "svg",
	"outputPath": null,
	"outputSuffix": null,
	"curlBaseUrl": "https://api.tvmaze.com/",
	"curlUrlAddon": null,
	"curlOptions": null,
	"curlUserAgentInformation": null,
	"curlSpanIdHeader": null,
	"curlTraceIdHeader": null,
	"summaryFile": "${name}",
	"input":  [
			{
				"inputFile": "show.json",
				"leadingObject": "show",
				"generateOutput": true,
				"mandatory": false,
				"curlBaseUrl": null,
				"curlCache": 0,
				"curlExecuteEvaluation": null,
				"curlFileSuffix": "_${id}",
				"curlFormatOutput": true,
				"curlOptions": null,
				"curlUrl": "shows/${id}?embed[]=cast&embed[]=nextepisode",
				"curlOutputParameter":  [
						{
							"${character}": "_embedded.cast.character.idx"
						},
						{
							"${name}": "name"
						},
						{
							"${person}": "_embedded.cast.person.idx"
						}
				],
				"splitIdentifier": null,
				"splitInputFile": false
			},
			{
				"inputFile": "person.json",
				"leadingObject": "person",
				"generateOutput": true,
				"mandatory": false,
				"curlBaseUrl": null,
				"curlCache": 0,
				"curlExecuteEvaluation": null,
				"curlFileSuffix": "_${person}",
				"curlFormatOutput": true,
				"curlOptions": null,
				"curlUrl": "people/${person}",
				"curlOutputParameter":  [
				],
				"splitIdentifier": null,
				"splitInputFile": false
			},
			{
				"inputFile": "character.json",
				"leadingObject": "character",
				"generateOutput": true,
				"mandatory": false,
				"curlBaseUrl": null,
				"curlCache": 0,
				"curlExecuteEvaluation": null,
				"curlFileSuffix": "_${character}",
				"curlFormatOutput": true,
				"curlOptions": null,
				"curlUrl": "character/${character}",
				"curlOutputParameter":  [
				],
				"splitIdentifier": null,
				"splitInputFile": false
			}
	]
}
